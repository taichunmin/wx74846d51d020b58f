    width: 100%;
}

page {
    background: #f9f9f9;
}

.outsideBlock {
    border: 1px solid #fff;
}

.outsideBlock,.outsideBlocks {
    background: #fff;
}

.block,.line {
    width: 100%;
}

.line {
    height: 160rpx;
    margin-top: 52rpx;
}

.lines,.right {
    height: 115rpx;
    width: 100%;
}

.right {
    float: left;
    line-height: 80rpx;
}

.title {
    color: #666;
    float: left;
    font-size: 30rpx;
    font-weight: 400;
    line-height: 115rpx;
    margin-left: 40rpx;
}

.rightIcon {
    float: right;
    margin-right: 36rpx;
}

.rightIcon,.upImg {
    height: 112rpx;
    width: 112rpx;
}

.upImg {
    border: 1px solid red;
}

.headPortrait,.headPortraits {
    border-radius: 50%;
    height: 112rpx;
    width: 112rpx;
}

.headPortraits {
    border: 1.5px solid #f9f9f9;
}

.none {
    background: #f9f9f9;
    height: 4rpx;
    width: 100%;
}

.rightIcons {
    float: right;
    height: 112rpx;
    margin-right: 36rpx;
}

.phoneNumber {
    float: left;
    margin-right: 15rpx;
}

.rightText {
    color: #666;
    font-size: 28rpx;
    font-weight: 400;
    line-height: 112rpx;
}

.iconBlock {
    float: left;
    margin-top: 25rpx;
}

.iconArrow {
    background: url("data:image/png;base64,");
    background-size: 40rpx 40rpx;
    height: 40rpx;
    width: 40rpx;
}

.btn {
    bottom: 170rpx;
    position: absolute;
    text-align: center;
    width: 100%;
}

.container-dialog-mask {
    background: #000;
    color: #fff;
    height: 100%;
    left: 0;
    opacity: .5;
    overflow: hidden;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 9000;
}

.container-dialog-content {
    background: #fff;
    border-radius: 16rpx;
    left: 0;
    margin: -200rpx 54rpx;
    overflow: hidden;
    position: fixed;
    top: 50%;
    width: 86%;
    z-index: 9999;
}

.txt-title {
    color: #000;
    font-size: 36rpx;
    font-weight: 500;
    padding-top: 50rpx;
    text-align: center;
}

.container-center-input {
    border-radius: 4rpx;
    display: flex;
    font-size: 28rpx;
    margin-left: 20rpx;
    padding: 50rpx 32rpx;
    width: 86%;
}

.imgBlock {
    background: url("data:image/png;base64,");
    background-size: 35rpx 35rpx;
    display: flex;
    height: 35rpx;
    margin-left: -20rpx;
    margin-top: 35rpx;
    width: 35rpx;
}

.input-dump-name {
    border-bottom: 1px solid #f1f1f1;
    box-sizing: border-box;
    color: #333;
    font-size: 34rpx;
    font-weight: 500;
    height: 96rpx;
    letter-spacing: 2rpx;
    line-height: 28rpx;
    padding: 0 20rpx;
    width: 100%;
}

.input-holder {
    color: #fff;
    font-size: 28rpx;
}

.phClass {
    font-size: 34rpx;
}

.container-bottom-btns {
    border-top: 1px solid #f1f1f1;
    display: flex;
    flex-direction: row;
    font-size: 34rpx;
    height: 86rpx;
    line-height: 86rpx;
}

.btn-cancel {
    border-right: 1px solid #f1f1f1;
    color: #020101;
}

.btn-cancel,.btn-confirm {
    flex: 1;
    text-align: center;
    width: 100%;
}

.btn-confirm {
    color: #f5a200;
}