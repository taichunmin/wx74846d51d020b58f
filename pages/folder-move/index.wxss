.topLine {
    background: rgba(0,0,0,.1);
    height: 2rpx;
    width: 100%;
}

.iconArrow {
    background: url("data:image/svg+xml;base64,");
    background-size: 40rpx 40rpx;
    height: 40rpx;
    width: 40rpx;
}

.main {
    flex: 1;
    overflow-y: scroll;
    position: relative;
}

.main-scroll {
    bottom: 0;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
}

.main-list {
    flex-wrap: wrap;
    justify-content: space-between;
    margin-left: 2vw;
    margin-right: 2vw;
}

.main-list,.no-folder-block {
    display: flex;
}

.no-folder-block {
    align-items: center;
    flex-direction: column;
    margin-top: 232rpx;
}

.no-folder-icon {
    background: url("data:image/png;base64,");
    background-size: 256rpx 256rpx;
    height: 256rpx;
    width: 256rpx;
}

.no-folder-text {
    color: #a8a8a8;
    font-size: 30rpx;
    margin-top: 30rpx;
}

.footer {
    background: #fff;
    color: #fff;
    flex: 0 0 168rpx;
}

.container-wallet-item-base {
    display: flex;
    flex-direction: row;
    margin: 60rpx 42rpx 0rpx 54rpx;
}

.container-cloud-disk-left {
    background: url("data:image/png;base64,");
    background-size: 90rpx 64rpx;
    height: 64rpx;
    width: 90rpx;
}

.container-folder-left {
    background: url("data:image/svg+xml;base64,");
    background-size: 90rpx 64rpx;
    height: 64rpx;
    width: 90rpx;
}

.container-wallet-item-right {
    display: flex;
    flex-direction: column;
    margin-left: 24rpx;
}

.container-wallet-item-right1 {
    align-items: center;
    display: flex;
    flex-direction: row;
    margin-left: 24rpx;
}

.txt-wallet-item-left-type {
    color: rgba(0,0,0,.1);
    font-size: 20rpx;
    font-weight: 700;
    height: 20rpx;
    left: 12rpx;
    position: relative;
    top: -10rpx;
    width: 18rpx;
}

.txt-wallet-item-right-name {
    color: #333;
    flex: 1;
    font-size: 30rpx;
    font-weight: bolder;
    margin-top: -8rpx;
}

.txt-wallet-item-right-type {
    align-items: flex-end;
    color: #757875;
    display: flex;
    flex: 1;
    font-size: 26rpx;
}

.container-wallet-item-slot {
    align-items: center;
    display: flex;
    flex: 1;
    justify-content: flex-end;
}

.icon-circle {
    background: url("data:image/png;base64,");
    background-size: 32rpx 32rpx;
    height: 32rpx;
    width: 32rpx;
}

.folder-icon {
    background: url("data:image/svg+xml;base64,");
    background-size: 90rpx 64rpx;
    height: 64rpx;
    width: 90rpx;
}

page {
    background: #f9f9f9;
}

.wraper,page {
    height: 100%;
    width: 100%;
}

.wraper {
    display: flex;
    flex-direction: column;
}

.header {
    flex: 0 0 0rpx;
}

.top-location {
    color: #666;
    font-size: 28rpx;
    margin-left: 46rpx;
    margin-top: 40rpx;
}

.tarbar-style {
    background: #fff;
    bottom: 0;
    height: 168rpx;
    position: fixed;
    width: 100%;
    z-index: 9999;
}

.tarbar-item-block {
    display: flex;
    flex: 1;
    flex-direction: row;
    margin-top: 18rpx;
}

.tarbar-item-block-no-checkbox {
    margin-top: 42rpx;
}

.tarbar-item-block-public-style {
    display: flex;
    flex: 1;
    flex-direction: column;
    font-size: 26rpx;
    justify-content: center;
}

.move-block {
    align-items: flex-start;
    color: #666;
    padding-left: 140rpx;
}

.delete-block {
    align-items: flex-end;
    color: #f56311;
    padding-right: 140rpx;
}

.tarbar-text {
    color: #666;
    font-size: 26rpx;
}

.wx-radio-input {
    height: 32rpx!important;
    margin-right: 4rpx!important;
    width: 32rpx!important;
}