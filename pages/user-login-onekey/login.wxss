.loginContent {
    margin-top: 140rpx;
}

.logo {
    height: 132px;
    margin-left: 62rpx;
    width: 201px;
}

.logoImg {
    background-size: cover;
    height: 100%;
    width: 100%;
}

.container-login-button {
    margin-left: 44rpx;
    margin-right: 44rpx;
    margin-top: 240rpx;
}

.btn-login-onekey {
    background: #f5a200;
    border-radius: 20px 20px 20px 20px;
    color: #fff;
    font-size: 30rpx;
    height: 40px;
    width: 100%;
}

.btn-login-onekey[disabled] {
    background: rgba(245,162,0,.5)!important;
    color: #fff!important;
}

.btn-login-onekey-hover {
    background: rgba(245,162,0,.8);
}

.btn-login-other-phone {
    background: hsla(0,0%,100%,.9);
    border: 1px solid #333;
    border-radius: 20px 20px 20px 20px;
    color: #333;
    font-size: 30rpx;
    margin-top: 26rpx;
    width: 100%;
}

button::after {
    border: none;
}

.txt-phone-mask {
    color: #000;
    font-size: 60rpx;
    font-weight: 700;
    line-height: 60rpx;
}

.txt-this-phone-login-provider {
    color: #999;
    font-size: 24rpx;
    font-weight: 400;
    line-height: 24rpx;
    margin-top: 20rpx;
}

.agree {
    bottom: 70rpx;
    height: auto;
    margin-right: 160rpx;
    position: absolute;
    text-align: center;
    width: 100%;
}

.viewRead {
    margin-left: 0rpx;
    margin-top: 0rpx;
    width: 100%;
}

.radio {
    height: 26rpx;
    left: 0rpx;
    position: relative;
    transform: scale(.8);
    width: 26rpx;
}

checkbox .wx-checkbox-input {
    border-radius: 50%;
    height: 46rpx;
    width: 46rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
    background: #1cd66c;
    border: 1rpx solid #1cd66c;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    background: transparent;
    border-radius: 50%;
    color: #fff;
    font-size: 30rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    transform: translate(-50%,-50%) scale(1);
    width: 40rpx;
}

@media screen and (max-width:320px) {
    .read {
        color: #444;
        font-weight: 400rpx;
        margin-left: 20rpx;
    }

    .privacy,.read {
        font-size: 34rpx;
    }
}

@media screen and (min-width:375px) {
    .read {
        color: #444;
        font-weight: 400rpx;
        margin-left: 20rpx;
    }

    .privacy,.read {
        font-size: 26rpx;
    }
}