page {
    height: 100%;
    margin: 0;
    padding: 0;
}

.container-base-view,page {
    background: #222426;
    overflow: hidden;
    width: 100%;
}

.container-base-view {
    display: flex;
    flex-direction: column;
    height: calc(100% - constant(safe-area-inset-bottom));
    height: calc(100% - env(safe-area-inset-bottom));
}

.container-layout-function {
    margin: 24rpx 24rpx 16rpx;
}

.container-layout-function,.txt-function-title {
    align-items: center;
    display: flex;
}

.txt-function-title {
    color: #fff;
    font-size: 30rpx;
    font-weight: 700;
    margin-left: 18rpx;
}

.container-layout-function icon {
    background-size: 34rpx 34rpx;
    height: 34rpx;
    width: 34rpx;
}

.icon-function-config {
    background: url("data:image/svg+xml;base64,");
}

.icon-function-response {
    background: url("data:image/svg+xml;base64,");
}

.icon-function-transmit {
    background: url("data:image/svg+xml;base64,");
}

.container-config-param-base {
    background: #222426;
    display: flex;
    flex-wrap: wrap;
}

.container-config-item {
    box-sizing: border-box;
    height: min-content;
    justify-content: center;
    padding: 20rpx;
    width: max-content;
}

.container-config-item,.txt-config-title {
    align-items: center;
    display: flex;
}

.txt-config-title {
    color: hsla(0,0%,84%,.9);
    font-size: 26rpx;
    font-weight: 400;
    height: 100%;
    line-height: 26rpx;
    margin-right: 12rpx;
}

.input-config-value {
    border-bottom: 2rpx solid hsla(0,0%,100%,.1);
    color: hsla(0,0%,84%,.9);
    width: 88rpx;
}

.container-tag-response-base {
    box-sizing: border-box;
    display: flex;
    flex: 1;
    flex-direction: column;
    height: 0rpx;
    overflow-y: auto;
    padding-left: 24rpx;
    padding-right: 24rpx;
}

.container-tag-transmit-base {
    height: auto;
}

.container-tag-transmit-base,.container-tag-transmit-top-input {
    display: flex;
    flex-direction: column;
    width: 100%;
}

.container-tag-transmit-top-input {
    box-sizing: border-box;
    padding: 24rpx;
}

.container-tag-transmit-bottom-btn {
    display: flex;
    flex-direction: row;
    width: 100%;
}

.container-tag-transmit-funtion {
    flex: 1;
    flex-direction: column;
    height: 100%;
    width: 0rpx;
}

.container-tag-transmit-btn,.container-tag-transmit-funtion {
    align-items: center;
    display: flex;
    justify-content: center;
}

.container-tag-transmit-btn {
    border-radius: 50%;
    box-shadow: 0rpx 0rpx 4rpx 4rpx hsla(0,0%,100%,.1);
    box-sizing: border-box;
    height: 128rpx;
    padding: 20rpx;
    width: 128rpx;
}

@-webkit-keyframes animated-border {
    0% {
        box-shadow: 0rpx 0rpx 4rpx 4rpx hsla(0,0%,100%,.1);
    }

    100% {
        box-shadow: 0rpx 0rpx 18rpx 18rpx hsla(0,0%,100%,.2);
    }
}

@keyframes animated-border {
    0% {
        box-shadow: 0rpx 0rpx 4rpx 4rpx hsla(0,0%,100%,.1);
    }

    100% {
        box-shadow: 0rpx 0rpx 18rpx 18rpx hsla(0,0%,100%,.2);
    }
}

.btn-transmit-2-tag-hover {
    animation: animated-border .1s;
    animation-fill-mode: forwards;
}

.btn-transmit-2-tag {
    background-size: 100% 100%;
    height: 100%;
    margin-right: 8rpx;
    width: 100%;
}

.input-tag-transmit-data {
    border-bottom: 2rpx solid hsla(0,0%,84%,.4);
    color: hsla(0,0%,84%,.9);
    width: 100%;
}

.container-tag-transmit-cmd-btns {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    height: 100%;
    justify-content: space-around;
    width: 100%;
}

.btn-transmit-cmd-fun {
    align-items: center;
    background: hsla(0,0%,100%,.1);
    border-radius: 30rpx;
    box-sizing: border-box;
    color: #fff;
    display: flex;
    font-size: 26rpx;
    font-weight: 400;
    height: min-content;
    justify-content: center;
    padding: 20rpx 30rpx;
}

.icon-cmd-list-expand {
    background: url("data:image/svg+xml;base64,");
    background-size: 28rpx 28rpx;
    height: 28rpx;
    margin-left: 8rpx;
    width: 28rpx;
}

.btn-transmit-cmd-fun-hover {
    animation: animated-border .1s;
    animation-fill-mode: forwards;
}

.container-response-item {
    border: 2rpx solid hsla(0,0%,100%,.1);
    box-sizing: border-box;
    display: flex;
    flex-wrap: wrap;
    margin-top: 20rpx;
    padding-left: 20rpx;
    padding-right: 20rpx;
}

.txt-response-item {
    color: hsla(0,0%,84%,.9);
    font-size: 26rpx;
    font-weight: 400;
    margin-bottom: 15rpx;
    margin-top: 15rpx;
    width: 50%;
}

.view-mask {
    background: rgba(0,0,0,.7);
    height: 100%;
    position: absolute;
    width: 100%;
    z-index: 1;
}

.btn-clear-recv-list {
    border: 2rpx solid hsla(0,0%,100%,.2);
    border-radius: 8rpx;
    color: #fff;
    font-size: 26rpx;
    font-weight: 400;
    line-height: 26rpx;
    margin-left: 10rpx;
    padding: 10rpx 20rpx;
}

.btn-clear-recv-list-hover {
    border: 2rpx solid hsla(0,0%,100%,.9);
    opacity: .7;
}

.container-auto-clear {
    align-items: center;
    color: #fff;
    display: flex;
    flex-direction: row;
    font-size: 26rpx;
    font-weight: 400;
    line-height: 26rpx;
    margin-left: auto;
}

.input-auto-clear-count {
    border-bottom: 2rpx solid hsla(0,0%,100%,.1);
    margin-left: 6rpx;
    width: 80rpx;
}