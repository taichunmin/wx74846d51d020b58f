page {
    background: #131418;
}

.wrap,page {
    height: 100%;
    width: 100%;
}

.wrap {
    overflow-y: auto;
}

.title {
    display: flex;
    flex: 1;
    flex-direction: column;
    width: 70%;
}

.data1 {
    color: #f5a200;
    margin-top: 40rpx;
}

.data1,.data2 {
    font-feature-settings: "tnum" 1;
    display: flex;
    font-family: -apple-system,Helvetica,sans-serif;
    font-size: 30rpx;
    font-weight: 700;
    justify-content: flex-start;
    padding-left: 36rpx;
    width: 90%;
}

.data2 {
    color: #fff;
    margin-top: 30rpx;
}

.remain-text {
    background: #2e3033;
    border-radius: 8rpx;
    color: #838b99;
    display: flex;
    font-size: 26rpx;
    font-weight: 400;
    justify-content: flex-start;
    margin: 30rpx 0rpx 0rpx 24rpx;
    padding: 4rpx 0rpx 4rpx 24rpx;
    width: 57%;
}

.remain-text-number {
    color: #cdcdcd;
}

.block {
    margin-left: 20rpx;
    margin-right: 20rpx;
    padding-bottom: 30rpx;
}

.block0Title {
    color: #838b99;
    font-size: 26rpx;
    font-weight: 500;
    height: 60rpx;
    padding-left: 12rpx;
    width: 100%;
}

.dataBlock {
    height: 90rpx;
    letter-spacing: 2rpx;
    width: 100%;
}

.block0Data1,.dataBlock {
    color: #fff;
    font-family: monospace;
    margin-left: 16rpx;
    margin-right: 40rpx;
}

.block0Data1 {
    font-size: 26rpx;
    font-weight: 500;
    width: 90%;
}

.rich_text_top {
    height: 1rpx;
    margin-top: 20rpx;
    width: 100%;
}

.number1 {
    color: #f5a200;
}

.identical {
    background: rgba(22,194,148,.2);
    border: 1px solid hsla(0,0%,100%,.2);
    border-radius: 4px 4px 4px 4px;
    color: #16c294;
    float: right;
    font-size: 20rpx;
    font-weight: 400;
    height: 40rpx;
    line-height: 40rpx;
    margin-top: -60rpx;
    text-align: center;
    width: 60rpx;
}

.line {
    margin: 40rpx 2.5% 25rpx;
    width: 95%;
}

.line,.line1 {
    background: #f0f0f0;
    height: 2rpx;
    opacity: .1;
}

.line1 {
    margin-top: 30rpx;
    width: 100%;
}