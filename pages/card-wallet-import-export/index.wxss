.content {
    background: url("data:image/svg+xml;base64,");
    background-size: 100% 382rpx;
    flex: 1;
    flex-direction: column;
    height: 382rpx;
    margin-top: 300rpx;
    width: 100%;
}

.btn,.content {
    align-items: center;
    display: flex;
}

.btn {
    flex: 1;
    flex-direction: row;
    justify-content: center;
    margin-top: 100rpx;
}

.remain-text {
    color: #666;
    font-size: 26rpx;
    font-weight: 500;
    margin-top: 76rpx;
}

.password-block {
    flex-direction: column;
    justify-content: center;
}

.input-block,.password-block {
    align-items: center;
    display: flex;
}

.input-block {
    background: #e9e9e9;
    border-radius: 2px 2px 2px 2px;
    justify-content: flex-start;
    margin-top: 30rpx;
    width: 88%;
}

.input {
    color: #f5a200;
    display: flex;
    flex: 0.95;
    font-size: 34rpx;
    font-weight: 700;
    letter-spacing: 1px;
    text-align: center;
}

.edit-password-block {
    margin-top: 26rpx;
    width: 100%;
}

.edit-password-block,.middle-content {
    align-items: center;
    display: flex;
    justify-content: center;
}

.middle-content {
    flex-direction: column;
    margin: 154rpx auto 0;
    width: 80%;
}

.link-title {
    color: #666;
    font-size: 26rpx;
}

.link {
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    color: #999;
    display: -webkit-box;
    font-size: 24rpx;
    margin-top: 20rpx;
    overflow: hidden;
    width: 100%;
    word-break: break-all;
}