page {
    background: #f9f9f9;
}

.icon-right-arrow {
    background: url("data:image/svg+xml;base64,");
    background-position: 50%;
    background-size: 40rpx 40rpx;
    height: 40rpx;
    width: 40rpx;
}

.settings-item-left-title {
    color: #333;
    font-size: 30rpx;
    font-weight: 400;
    line-height: 30rpx;
}

.settings-item-right-container {
    flex: 1;
    justify-content: flex-end;
}

.settings-item-right-container,.settings-item-row-container {
    align-items: center;
    display: flex;
    flex-direction: row;
    width: 100%;
}

.settings-item-base-container {
    align-items: flex-start;
    background: #fff;
    border: 1px solid #f9f9f9;
    box-sizing: border-box;
    color: #333;
    display: flex;
    flex-direction: column;
    padding: 26rpx 30rpx;
    width: 100%;
}

.upContent {
    height: auto;
    width: 100%;
}

.item {
    color: #333333c7;
    font-size: 24rpx;
    font-weight: 500;
    margin-left: 6rpx;
    word-break: break-all;
}

.sheet_title {
    color: #333;
    font-size: 34rpx;
    font-weight: 600;
}

.location,.sheet_title {
    border: 1px solid #f9f9f9;
    height: 112rpx;
    line-height: 112rpx;
    text-align: center;
    width: 99%;
}

.location {
    color: #000;
    font-weight: 400;
}

.emptyBlock {
    background: #f2f2f2;
    height: 14rpx;
    width: 100%;
}

.cancel {
    border: 1px solid #f9f9f9;
    color: #f56311;
    font-weight: 400;
    height: 112rpx;
    line-height: 112rpx;
    margin-bottom: 50rpx;
    text-align: center;
    width: 99%;
}