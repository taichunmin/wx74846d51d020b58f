@-webkit-keyframes overview-mp-dump-to-device-animation {
    0% {
        height: 0;
        width: 0;
    }

    100% {
        height: 38px;
        width: 120px;
    }
}

@keyframes overview-mp-dump-to-device-animation {
    0% {
        height: 0;
        width: 0;
    }

    100% {
        height: 38px;
        width: 120px;
    }
}

.container-mp-dump-to-device-overview {
    align-items: center;
    animation-duration: .2s;
    animation-name: overview-mp-dump-to-device-animation;
    background: #f5a200;
    border: 4rpx solid hsla(0,0%,100%,.3);
    border-radius: 20rpx;
    display: flex;
    height: 38px;
    justify-content: center;
    position: absolute;
    width: 140px;
}

.txt-tag-slot-item-right-name {
    color: #333;
    flex: 1;
    font-size: 30rpx;
    font-weight: bolder;
}

.line-chameleon-slot-item {
    background: #f9f9f9;
    height: 16rpx;
    width: 100%;
}