.container-page-base {
    display: flex;
    flex-direction: column;
    height: 100%;
    width: 100%;
}

.container-line {
    background: #f9f9f9;
    height: 16rpx;
    margin-top: 6rpx;
    width: 100%;
}

.container-dump-list {
    display: flex;
    flex: 1;
    flex-direction: column;
    overflow: scroll;
    width: 100%;
}

.container-dump-item {
    padding: 30rpx;
}

page {
    height: 100%;
    width: 100%;
}

.container-fucntion-item-base {
    display: flex;
    flex-direction: row;
    margin-top: 10rpx;
    padding: 10rpx 50rpx;
    width: 100%;
}

.icon-dump-manager-pc {
    background: url("data:image/svg+xml;base64,");
    background-size: 80rpx 80rpx;
    height: 80rpx;
    width: 80rpx;
}

.icon-keys-manager {
    background: url("data:image/svg+xml;base64,");
    background-size: 80rpx 80rpx;
    height: 80rpx;
    width: 80rpx;
}

.icon-import-dump {
    background: url("data:image/svg+xml;base64,");
    background-size: 80rpx 80rpx;
    height: 80rpx;
    width: 80rpx;
}

.container-function-item-right-title {
    box-sizing: border-box;
    display: flex;
    flex: 1;
    flex-direction: column;
    margin-left: 24rpx;
    padding: 14rpx 0;
}

.txt-function-item-main-title {
    color: #333;
    flex: 1;
    font-size: 28rpx;
    font-weight: 400;
    line-height: 28rpx;
}

.txt-function-item-sub-title {
    color: #999;
    flex: 1;
    font-size: 24rpx;
    font-weight: 400;
    line-height: 24rpx;
    margin-top: 10rpx;
}

.function-button-item-hover {
    background: rgba(51,51,51,.06);
}

.container-dump-import-title {
    font-size: 40rpx;
    font-weight: blod;
    margin: 30rpx;
}

.container-dump-import-center {
    flex: 1;
    overflow: scroll;
}

.container-dump-import-btns {
    align-items: center;
    border-top: 1rpx solid rgba(3,3,3,.05);
    box-sizing: border-box;
    display: flex;
    justify-content: center;
    text-align: center;
    width: 100%;
}

.btn-data-dump-import {
    flex: 1;
    font-size: 28rpx;
    padding: 26rpx 0;
}

.btn-data-dump-confirm {
    color: #f5a200;
}

.btn-data-dump-cancel {
    border-right: 1rpx solid rgba(3,3,3,.05);
    color: #333;
}

.container-import-info-input-item {
    align-items: center;
    border-bottom: 1rpx solid rgba(51,51,51,.1);
    box-sizing: border-box;
    color: #222;
    display: flex;
    flex-direction: row;
    font-size: 28rpx;
    font-weight: 700;
    margin: 60rpx 30rpx;
    padding-bottom: 10rpx;
}

