.header_view_hide {
    display: none;
}

.header_view_show {
    display: block;
}

.remainingTimes {
    background: #f56311;
    border-radius: 20rpx 0rpx 20rpx 0rpx;
    color: #fff;
    font-size: 22rpx;
    height: 36rpx;
    line-height: 36rpx;
    width: 128rpx;
}

.IC {
    background-color: #231815;
}

.IC,.ID {
    border-radius: 6rpx 6rpx 6rpx 6rpx;
    color: #fff;
    font-size: 36rpx;
    font-weight: 600;
    height: 18%;
    margin: 24rpx auto 0;
    width: 10%;
}

.ID {
    background-color: #9f9f9f;
}

.tips {
    border-radius: 20rpx 20rpx 20rpx 20rpx;
    color: #a9a9a9;
    font-size: 20rpx;
    font-weight: 400;
    margin: 20rpx auto 0;
    text-align: center;
    width: 80%;
}

.search {
    color: #333;
    font-size: 34rpx;
    font-weight: 600;
    margin-top: 15%;
    padding-left: 0;
    position: relative;
}

page {
    height: 100%;
}

.readCard {
    height: 500rpx;
    width: 370rpx;
}

.readCardBlock {
    height: 450rpx;
    margin: 0 auto;
    text-align: center;
    width: 350rpx;
}

.progress {
    margin-left: 44rpx;
    margin-right: 44rpx;
    margin-top: 72rpx;
}

.progress-bar-box {
    background: #ebedf0;
}

.progress-bar-box,.progress-bar-in {
    border: 0;
    border-radius: 8rpx;
    height: 360rpx;
}

.progress-bar-in {
    background: #f5a200;
    transition: all 2s ease 0s;
    width: 1rpx;
}

.cardReadText {
    margin-top: 60rpx;
}

.readCardTime {
    color: #333;
    font-size: 34rpx;
    font-weight: 600;
    height: 50rpx;
    letter-spacing: 2rpx;
    margin: 0 auto;
    text-align: center;
    width: 85%;
}

.remain {
    color: #757875;
    font-size: 26rpx;
    font-weight: 400;
    margin-top: 24rpx;
    position: absolute;
}

.remain,.remainBlock {
    left: 0;
    text-align: center;
    width: 100%;
}

.remainBlock1 {
    left: 0;
    width: 85%;
}

.remainBlock1,.tipsText {
    margin: 20rpx auto 0;
    text-align: center;
}

.tipsText {
    color: #757875;
    font-size: 26rpx;
    font-weight: 400;
    width: 80%;
}

.content {
    margin-top: 100rpx;
    text-align: center;
}

.success {
    color: #333;
    font-size: 34rpx;
    font-weight: bolder;
    font-weight: 600;
    letter-spacing: 1px;
    margin-top: 30rpx;
}

.fail {
    background: url("data:image/svg+xml;base64,");
    background-size: 200rpx 200rpx;
    height: 200rpx;
    width: 200rpx;
}

.doubleBtn {
    bottom: 200rpx;
    position: absolute;
    width: 100%;
}

.leftBtn {
    float: left;
    margin-left: 40rpx;
}

.leftBtn,.rightBtn {
    height: 40%;
    width: 40%;
}

.rightBtn {
    float: right;
    margin-right: 40rpx;
}

button::after {
    border: none;
}

.panel {
    margin-bottom: 148rpx;
}

button[disabled] {
    background: #f8f8ff!important;
    color: #f5a200!important;
}

.prohibit {
    background: #f7f7f7;
    border: 1px solid #fff;
    border-radius: 20px 20px 20px 20px;
    color: #757875;
}

.noProhibit,.prohibit {
    font-size: 30rpx;
    font-weight: 500;
    width: 100%;
}

.noProhibit {
    background: #fff;
    border: 1px solid #f5a200;
    border-radius: 20px 20px 20px 20px;
    color: #f5a200;
}

.successCardNumber {
    color: #757875;
    font-size: 26rpx;
    height: 60rpx;
    margin-top: 20rpx;
    text-align: center;
    width: 100%;
}

.ic-info {
    background: #f9f9f9;
    border-radius: 8rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    margin: 24rpx auto 0;
    padding-top: 24rpx;
    width: 80%;
}

.ic-info-left,.ic-info-right {
    display: flex;
    flex-direction: column;
}

.ic-info-right {
    align-items: flex-start;
    justify-content: flex-start;
}

.ic-info-top {
    margin: 0rpx 0rpx 12rpx 30rpx;
    text-align: left;
}

.ic-info-top,.ic-info-top-text {
    font-size: 24rpx;
    font-weight: 500;
}

.ic-info-top-text {
    color: #f5a200;
}

.ic-info-top-text-left {
    margin: 0rpx 0rpx 12rpx;
}

.id-info-top-text {
    color: #008ef5;
}

.card-information {
    border: 1px solid blue;
    padding-right: 200rpx;
    text-align: right;
    width: 300rpx;
}