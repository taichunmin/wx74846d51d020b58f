.topLine {
    background: rgba(0,0,0,.1);
    height: 2rpx;
    width: 100%;
}

.img-share-btn-wechat {
    background: url("data:image/svg+xml;base64,");
    background-size: 120rpx 120rpx;
    height: 120rpx;
    width: 120rpx;
}

.img-share-btn-link {
    background: url("data:image/svg+xml;base64,");
    background-size: 120rpx 120rpx;
    height: 120rpx;
    width: 120rpx;
}

page {
    height: calc(100% - constant(safe-area-inset-bottom));
    height: calc(100% - env(safe-area-inset-bottom));
    width: 100%;
}

.container-page-base {
    display: flex;
    flex-direction: column;
    height: 100%;
    overflow: hidden;
    width: 100%;
}

.container-share-list {
    background: #f9f9f9;
    flex: 1;
    overflow: scroll;
    width: 100%;
}

.container-wallet-item-base {
    background-color: #fff;
    display: flex;
    flex-direction: column;
    margin-top: 16rpx;
    padding: 44rpx 24rpx 0rpx 30rpx;
}

.card-info,.container-shared-btns {
    display: flex;
    flex-direction: row;
}

.container-shared-btns {
    align-items: center;
    border-top: 1px solid #f0f0f0;
    margin-top: 42rpx;
}

.container-wallet-item-left {
    border-radius: 4rpx;
    height: 100rpx;
    width: 168rpx;
}

.container-wallet-item-right {
    display: flex;
    flex-direction: column;
    margin-left: 24rpx;
}

.txt-wallet-item-left-type {
    color: rgba(0,0,0,.1);
    display: flex;
    font-size: 20rpx;
    font-weight: 700;
    padding: 12rpx 0rpx 0rpx 12rpx;
}

.txt-wallet-item-right-name {
    color: #333;
    flex: 1;
    font-size: 30rpx;
    font-weight: bolder;
}

.txt-wallet-item-right-type {
    align-items: flex-end;
    color: #757875;
    display: flex;
    flex: 1;
    font-size: 26rpx;
}

.container-wallet-item-slot {
    align-items: center;
    display: flex;
    flex: 1;
    justify-content: flex-end;
}

.btn-shared-btn-base {
    color: #666;
    flex: 0.4;
    font-size: 24rpx;
    padding-bottom: 24rpx;
    padding-top: 24rpx;
}

.btn-shared-btn-base,.middle-line {
    align-items: center;
    display: flex;
    justify-content: center;
}

.middle-line {
    color: #f0f0f0;
    flex: 0.2;
    height: 20rpx;
}

.cancel-shared-disabled {
    color: hsla(0,0%,40%,.3);
    display: flex;
    flex: 0.4;
    font-size: 24rpx;
    justify-content: center;
}

.noneCard {
    height: auto;
    margin-top: 232rpx;
    text-align: center;
    width: 100%;
}

.noneCardImg {
    height: 288rpx;
    width: 300rpx;
}

.noneCardText {
    color: #a8a8a8;
    font-size: 30rpx;
    font-weight: 500;
}

.container-share-panel {
    height: 100%;
    padding-top: 38rpx;
}

.container-share-action,.container-share-panel {
    display: flex;
    flex-direction: column;
    width: 100%;
}

.container-share-action {
    justify-content: flex-end;
}

.container-share-to-who {
    align-items: center;
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.line-share-to-who {
    border: 1rpx solid hsla(0,0%,91%,.8);
    height: 0rpx;
    opacity: 1;
    width: 110rpx;
}

.txt-share-to {
    color: #666;
    font-size: 24rpx;
    font-weight: 400;
    line-height: 24rpx;
    margin-left: 24rpx;
    margin-right: 24rpx;
}

.container-share-btn-group {
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    width: 100%;
}

.container-share-btn-item {
    align-items: center;
    display: flex;
    flex: 1;
    flex-direction: column;
    margin-top: 60rpx;
}

button {
    background: none;
}

button::after {
    border: none;
}

.button-hover {
    background: none;
}

.txt-share-btn {
    color: #666;
    font-size: 24rpx;
    font-weight: 400;
    line-height: 24rpx;
    margin-top: 24rpx;
}

.btn-share-cancel {
    border-bottom: 1rpx solid hsla(220,8%,47%,.06);
    border-top: 2rpx solid #f0f0f0;
    color: #666;
    font-size: 28rpx;
    font-weight: 400;
    line-height: 28rpx;
    margin-top: 30rpx;
    padding-bottom: 40rpx;
    padding-top: 40rpx;
    text-align: center;
    width: 100%;
}

.container-append-access-code {
    box-sizing: border-box;
    flex-direction: row;
    justify-content: center;
    padding-left: 130rpx;
    padding-right: 130rpx;
    padding-top: 60rpx;
    width: 100%;
}

.container-append-access-code,.txt-share-auto-append-ac {
    align-items: center;
    display: flex;
    font-size: 28rpx;
}

.txt-share-auto-append-ac {
    color: #6e7480;
    flex: 1;
    font-weight: 400;
    height: 100%;
    line-height: 28rpx;
    margin-bottom: 10rpx;
}