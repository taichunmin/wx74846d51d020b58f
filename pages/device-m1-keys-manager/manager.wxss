page {
    margin: 0;
    padding: 0;
}

.main-container,page {
    background: #131418;
    height: 100%;
    overflow: hidden;
    width: 100%;
}

.main-container {
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
}

.top-warn-message-container {
    background: rgba(245,162,0,.1);
    color: #f5a200;
    font-size: 26rpx;
    font-weight: 500;
    line-height: 26rpx;
    margin: 20rpx 20rpx 0rpx;
    padding-bottom: 12rpx;
    padding-left: 28rpx;
    padding-top: 12rpx;
}

.top-tab-container {
    background: hsla(0,0%,100%,.1);
    border-radius: 50px 50px 50px 50px;
    display: flex;
    height: 70rpx;
    margin: 20rpx auto 0;
    width: 90%;
}

.top-tab-item {
    color: #fff;
    font-size: 26rpx;
    font-weight: 500;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    width: 332rpx;
}

.top-tab-item-choose {
    background: hsla(0,0%,100%,.4);
    border-radius: 50px 50px 50px 50px;
}

.top-tab-title {
    color: #fff;
    font-size: 26rpx;
    font-weight: 500;
    line-height: 70rpx;
}

.keys-base-main-container,.keys-base-tab-page-container {
    flex: 1;
    height: 100%;
    overflow: hidden;
    width: 100%;
}

.keys-base-tab-page-container {
    display: flex;
    flex-direction: column;
}

.keys-base-info-container {
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    padding: 40rpx 30rpx 20rpx;
    width: 100%;
}

.keys-reading-use-enable {
    flex: 1;
    text-align: right;
}

.keys-base-info-title,.keys-reading-use-enable {
    align-items: center;
    justify-content: center;
}

.keys-base-info-title {
    color: #838b99;
    display: flex;
    font-size: 24rpx;
    font-weight: 400;
}

.bottom-button-container {
    display: flex;
    flex-direction: row;
    justify-content: center;
    margin-bottom: env(safe-area-inset-bottom);
    padding-bottom: 20rpx;
    padding-top: 20rpx;
    width: 100%;
}

.bottom-button-item {
    flex: 1;
    margin: 0rpx 30rpx;
}

.clear-btn {
    flex: 0.3;
    margin: 0rpx 30rpx;
}

.right-btn-block {
    align-items: center;
    background: #f5a200;
    border-radius: 40rpx;
    display: flex;
    flex: 0.7;
    flex-direction: row;
    justify-content: center;
}

.middle-line {
    color: #fff;
    margin: 0rpx 35rpx;
}

.bottom-button-clear {
    background: #131418;
    border: 1px solid #6e7480;
    border-radius: 20px 20px 20px 20px;
    color: hsla(0,0%,100%,.6);
}

.bottom-button-add,.bottom-button-clear {
    font-size: 30rpx;
    font-weight: 500;
    width: 100%;
}

.bottom-button-add {
    background: #f5a200;
    border: #f5a200;
    border-radius: 20px 20px 20px 20px;
    color: #fff;
    opacity: 1;
    padding: 3rpx 0rpx;
}

.keys-table-head {
    border-radius: 0rpx 0rpx 0rpx 0rpx;
    border-top: 2rpx solid hsla(0,0%,100%,.1);
    margin-left: 24rpx;
    margin-right: 24rpx;
}

.right-keys-history-cell {
    background: #131418;
    display: flex;
    flex-direction: column;
    height: 90%;
    width: 80%;
}

.right-keys-history-title {
    background: #131418;
    color: #fff;
    padding: 30rpx;
    text-align: center;
}

.colorText {
    color: #f5a200;
}

.van-action-sheet__cancel,.van-action-sheet__item,.van-popup {
    background-color: #1a1626!important;
    color: #fff;
}

.van-action-sheet__gap {
    background-color: #fafafa!important;
    height: 0rpx!important;
}