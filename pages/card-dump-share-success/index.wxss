.img-share-btn-wechat {
    background: url("data:image/svg+xml;base64,");
    background-size: 120rpx 120rpx;
    height: 120rpx;
    width: 120rpx;
}

.img-share-btn-link {
    background: url("data:image/svg+xml;base64,");
    background-size: 120rpx 120rpx;
    height: 120rpx;
    width: 120rpx;
}

.container-page-base {
    align-items: center;
    display: flex;
    flex-direction: column;
    height: 100%;
    width: 100%;
}

.icon-share-create-success {
    background: url("data:image/svg+xml;base64,");
    background-size: 200rpx 200rpx;
    height: 200rpx;
    margin-top: 200rpx;
    width: 200rpx;
}

.txt-share-create-success {
    color: #333;
    font-size: 34rpx;
    font-weight: 700;
    letter-spacing: 2rpx;
    line-height: 40rpx;
    margin-top: 30rpx;
}

.container-share-info {
    background: #f9f9f9;
    box-sizing: border-box;
    margin-top: 24rpx;
    padding: 30rpx;
    width: 85%;
}

.txt-share-dump-name {
    color: #333;
    font-size: 28rpx;
    font-weight: 500;
    line-height: 28rpx;
}

.txt-share-dump-info {
    color: #666;
    display: flex;
    flex-direction: row;
    font-size: 26rpx;
    font-weight: 400;
    height: 26rpx;
    line-height: 26rpx;
    margin-top: 20rpx;
}

.container-share-action {
    display: flex;
    flex: 1;
    flex-direction: column;
    justify-content: flex-end;
}

.container-share-to-who {
    align-items: center;
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.line-share-to-who {
    border: 1rpx solid hsla(0,0%,91%,.8);
    height: 0rpx;
    opacity: 1;
    width: 110rpx;
}

.txt-share-to {
    color: #666;
    font-size: 24rpx;
    font-weight: 400;
    line-height: 24rpx;
    margin-left: 24rpx;
    margin-right: 24rpx;
}

.container-share-btn-group {
    display: flex;
    flex-direction: row;
}

.container-share-btn-item {
    align-items: center;
    display: flex;
    flex-direction: column;
    margin-top: 60rpx;
}

button {
    background: none;
}

button::after {
    border: none;
}

.button-hover {
    background: none;
}

.txt-share-btn {
    color: #666;
    font-size: 24rpx;
    font-weight: 400;
    line-height: 24rpx;
    margin-top: 24rpx;
}