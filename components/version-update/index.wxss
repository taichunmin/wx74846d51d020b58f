.wrapper {
    align-items: center;
    height: 100%;
    justify-content: center;
    margin: 0 auto;
    width: 87%;
}

.upBlock,.wrapper {
    display: flex;
}

.upBlock {
    background-color: #fff;
    border-radius: 6px 6px 6px 6px;
    flex-direction: column;
    height: auto;
    width: 90%;
}

.title {
    align-items: center;
    background: #f5a200;
    border-radius: 6px 6px 0 0;
    color: #fff;
    display: flex;
    font-size: 34rpx;
    font-weight: 700;
    height: 100rpx;
    justify-content: center;
    letter-spacing: 2rpx;
    width: 100%;
}

.upContent {
    height: auto;
    margin-left: 30rpx;
    padding-top: 40rpx;
    width: 100%;
}

.item {
    color: #333;
    font-size: 28rpx;
    font-weight: 400;
    margin-left: 40rpx;
    margin-right: 10rpx;
    text-indent: -30rpx;
    width: 85%;
    word-break: break-all;
}

.item,.upBtn {
    margin-bottom: 20rpx;
}

.upBtn {
    height: auto;
    margin-top: 80rpx;
    text-align: center;
    width: 100%;
}

.noProhibit {
    background: #fff;
    border: 1px solid #f5a200;
    border-radius: 20px 20px 20px 20px;
    color: #f5a200;
    font-size: 30rpx;
    font-weight: 500;
    width: 50%;
}

button[disabled] {
    background: #f8f8ff!important;
    color: #f5a200!important;
}

.upCheck {
    color: #999;
    font-size: 28rpx;
    height: auto;
    margin-bottom: 30rpx;
    margin-top: 15rpx;
    text-align: center;
    width: 100%;
}