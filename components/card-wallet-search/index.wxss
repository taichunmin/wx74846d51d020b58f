.input_search_bar {
    background: #f8faf9;
    height: 100%;
    width: 100%;
}

.input_search_bar .input_search_bar_form {
    -webkit-box-flex: 1;
    background: var(--viewBackgroundColor);
    border-radius: 15rpx;
    flex: auto;
    position: relative;
}

.input_search_bar .input_search_bar_form .input_search_bar_box {
    align-items: center;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    justify-content: center;
    padding-right: 60rpx;
    width: 100%;
    z-index: 1;
}

.input_search_bar .input_search_bar_form .input_search_bar_box .input_search_bar_input {
    display: flex;
    flex: 1;
    font-size: 30rpx;
    height: 50rpx;
    line-height: 50rpx;
    padding-left: 13rpx;
}

.input_search_bar .input_search_bar_form .input_search_bar_box .order_icon_clear {
    display: flex;
    margin-right: -40rpx;
}

.input_search_bar .input_search {
    color: #f5a200;
    font-weight: 500;
}

.input_search_bar .input_cancel,.input_search_bar .input_search {
    font-size: 30rpx;
    line-height: 28rpx;
    margin-top: 12rpx;
    white-space: nowrap;
}

.input_search_bar .input_cancel {
    color: #666;
    margin-left: 12rpx;
}

.searchIcon {
    background: url("data:image/svg+xml;base64,");
    background-size: 32rpx 32rpx;
    display: flex;
    height: 32rpx;
    margin-left: 14rpx;
    width: 32rpx;
}

.container-custom-toolbar {
    display: flex;
    flex: 1;
    flex-direction: row;
    width: 100%;
}

.container-toolbar-title {
    color: #000;
    font-size: 34rpx;
    font-weight: 500;
    margin-right: -20rpx;
    min-width: 156rpx;
    padding-left: 30rpx;
    padding-top: -10rpx;
}

.container-search-block,.container-toolbar-title {
    align-items: center;
    display: flex;
}

.container-search-block {
    margin-left: 28rpx;
    margin-right: 28rpx;
}