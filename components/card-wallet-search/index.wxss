.container {
    align-items: center;
    flex-direction: row;
    height: 64rpx;
    width: 100%;
}

.container,.input_search_bar {
    display: flex;
    flex: 1;
}

.input_search_bar {
    background: #f8faf9;
    border-radius: 40rpx 40rpx 40rpx 40rpx;
}

.cancel-text-block {
    color: #999;
    display: flex;
    font-size: 26rpx;
    justify-content: flex-end;
    margin-left: 20rpx;
    margin-right: 18rpx;
    max-width: 90rpx;
}

.input_search_bar .input_search_bar_form {
    -webkit-box-flex: 1;
    background: var(--viewBackgroundColor);
    border-radius: 15rpx;
    flex: auto;
    position: relative;
}

.input_search_bar .input_search_bar_form .input_search_bar_box {
    align-items: center;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    justify-content: center;
    padding-right: 60rpx;
    width: 100%;
    z-index: 1;
}

.input_search_bar .input_search_bar_form .input_search_bar_box .input_search_bar_input {
    display: flex;
    font-size: 30rpx;
    height: 64rpx;
    line-height: 64rpx;
    padding-left: 13rpx;
    width: 100%;
}

.input_search_bar .input_search_bar_form .input_search_bar_box .order_icon_clear {
    display: flex;
    margin-right: -40rpx;
}

.input_search_bar .input_search {
    color: #f5a200;
    font-weight: 500;
}

.input_search_bar .input_cancel,.input_search_bar .input_search {
    font-size: 30rpx;
    line-height: 28rpx;
    margin-top: 12rpx;
    white-space: nowrap;
}

.input_search_bar .input_cancel {
    color: #666;
    margin-left: 12rpx;
}

.searchIcon {
    background: url("data:image/svg+xml;base64,");
    background-size: 32rpx 32rpx;
    display: flex;
    height: 32rpx;
    margin-left: 14rpx;
    width: 32rpx;
}

.icon-clear {
    background: url("data:image/png;base64,");
    background-size: 36rpx 36rpx;
    height: 36rpx;
    width: 36rpx;
}