.keys-base-main-container {
    display: flex;
    flex-direction: column;
    height: 100%;
    overflow: hidden;
    width: 100%;
}

.keys-base-data-container {
    display: flex;
    flex: 1;
    flex-direction: column;
    height: 0rpx;
    overflow: auto;
    width: 100%;
}

.keys-table-row {
    background: #131418;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    min-height: 80rpx;
}

.keys-table-data-odd-style:nth-child(odd) {
    background: #1a1d26;
    border-radius: 0 0 0 0;
    opacity: 1;
}

.keys-table-head-text {
    color: #fff;
    font-size: 26rpx;
    line-height: 26rpx;
}

.keys-table-data,.keys-table-head-text {
    align-items: center;
    display: flex;
    flex: 1;
    font-family: siyuan,monospace;
    font-weight: 400;
    justify-content: center;
    width: 100%;
}

.keys-table-data {
    color: #f5f5f5;
    font-size: 24rpx;
    line-height: 24rpx;
}

.icon-item-edit {
    background: url("data:image/svg+xml;base64,");
    background-size: 30rpx 30rpx;
    height: 30rpx;
    width: 30rpx;
}

.icon-item-delete {
    background: url("data:image/svg+xml;base64,");
    background-size: 30rpx 30rpx;
    height: 30rpx;
    width: 30rpx;
}

.keys-opera-button {
    align-items: center;
    box-sizing: border-box;
    display: flex;
    height: 100%;
    justify-content: center;
    padding-left: 20rpx;
    padding-right: 20rpx;
}

.right-button-item-hover {
    background: #404346;
}

.keys-opera-button-line {
    border-left: 2rpx solid hsla(0,0%,100%,.1);
    box-sizing: border-box;
    height: 30%;
    margin-top: 6rpx;
}