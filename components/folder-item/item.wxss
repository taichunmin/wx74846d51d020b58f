.folder-icon {
    background: url("data:image/svg+xml;base64,");
    background-size: 90rpx 64rpx;
    height: 64rpx;
    width: 90rpx;
}

.container-wallet-item-base {
    display: flex;
    flex: 1;
    flex-direction: row;
    height: 64rpx;
}

.checkbox-block {
    align-items: center;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.container-wallet-item-left {
    border-radius: 4rpx;
    height: 64rpx;
    margin-bottom: 0rpx;
    margin-left: 0rpx;
    margin-top: 0rpx;
    width: 90rpx;
}

.container-wallet-item-right {
    display: flex;
    flex: 1;
    flex-direction: column;
}

.txt-wallet-item-right-name {
    color: #333;
    font-size: 26rpx;
    font-weight: bolder;
    margin-top: -6rpx;
    min-height: 32rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 170rpx;
}

.txt-wallet-item-right-type-pub {
    align-items: flex-end;
    color: #999;
    display: flex;
    flex-direction: row;
}

.txt-wallet-item-right-type {
    margin-top: 8rpx;
}

.txt-wallet-item-right-type1 {
    margin-top: 2rpx;
}

.type {
    font-size: 24rpx;
}

.path {
    font-size: 20rpx;
    margin-left: 10rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 170rpx;
}

.container-wallet-item-slot {
    align-items: center;
    display: flex;
    flex: 1;
    flex-direction: row;
    justify-content: flex-end;
}

.file-ID {
    background: url("data:image/svg+xml;base64,");
    background-size: 90rpx 64rpx;
}

.file-IC,.file-ID {
    height: 64rpx;
    width: 90rpx;
}

.file-IC {
    background: url("data:image/svg+xml;base64,");
    background-size: 90rpx 64rpx;
}