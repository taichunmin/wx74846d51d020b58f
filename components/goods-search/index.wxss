.topLine {
    background: rgba(0,0,0,.1);
    height: 2rpx;
    width: 100%;
}

.container {
    background: #fff;
    margin: 0;
    position: fixed;
    width: 100%;
    z-index: 9999;
}

.empty-block {
    background: #fff;
    height: 12rpx;
}

.input_search_bar {
    height: 64rpx;
    width: 100%;
}

.within-block {
    align-items: center;
    background: #f8faf9;
    border-radius: 10rpx 10rpx 10rpx 10rpx;
    flex-direction: row;
    height: 100%;
    margin: 0 auto;
    width: 95%;
}

.search-icon-block,.within-block {
    display: flex;
}

.search-icon-block {
    flex: 0.1;
    justify-content: center;
}

.search-input-block {
    display: flex;
    flex: 0.8;
}

.clear-icon-block {
    flex: 0.1;
}

.clear-icon-block,.searchIcon {
    display: flex;
}

.searchIcon {
    background: url("data:image/svg+xml;base64,");
    background-size: 32rpx 32rpx;
    height: 32rpx;
    margin-left: 14rpx;
    width: 32rpx;
}