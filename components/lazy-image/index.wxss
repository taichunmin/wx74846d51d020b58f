@font-face {
    font-family: iconfont;
    src: url("data:application/x-font-woff2;charset=utf-8;base64,") format("woff2"),url("//at.alicdn.com/t/font_2717605_k3xtrlb30s8.woff?t=1627956181545") format("woff"),url("//at.alicdn.com/t/font_2717605_k3xtrlb30s8.ttf?t=1627956181545") format("truetype");
}

.iconfont {
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    font-family: iconfont!important;
    font-size: 16px;
    font-style: normal;
}

.icon-image-err:before {
    content: "\e63d";
}

.icon-image:before {
    content: "\e649";
}

.lazy-image-wrap {
    height: 100%;
    position: relative;
    width: 100%;
}

.lazy-image {
    background-repeat: no-repeat;
    background-size: 100% 100%;
    opacity: 0;
    transition: opacity .5s;
}

.lazy-image-wrap .lazy-image.show {
    opacity: 1;
}

.lazy-image-loading {
    align-items: center;
    background-color: #f7f8fa;
    bottom: 0;
    color: #969799;
    display: flex;
    height: 100%;
    justify-content: center;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
    width: 100%;
}