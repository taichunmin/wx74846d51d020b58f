.top-icon {
    align-items: center;
    display: flex;
    flex-direction: row;
    padding: 34rpx 38rpx 0rpx 0rpx;
}

.card-search {
    margin-left: 20rpx;
    margin-top: 30rpx;
    width: 97%;
}

.no-card {
    align-items: center;
    display: flex;
    flex-wrap: wrap;
    height: 100%;
    justify-content: center;
}

.icon-current-url {
    background: url("data:image/svg+xml;base64,");
    background-size: 40rpx 40rpx;
    height: 40rpx;
    width: 40rpx;
}

.current-url-title-block {
    align-items: center;
    display: flex;
    flex: 1;
    flex-direction: row;
    justify-content: flex-start;
    margin-left: 34rpx;
}

.current-url-title {
    margin-left: 24rpx;
}

.current-url-title-right-icon {
    align-items: center;
    display: flex;
    flex: 1;
    flex-direction: row;
    justify-content: flex-end;
}

.icon-screen {
    background: url("data:image/svg+xml;base64,");
    background-size: 39.76rpx 32.07rpx;
    height: 32.07rpx;
    width: 39.76rpx;
}

.icon-search {
    background: url("data:image/svg+xml;base64,");
    background-size: 50rpx 50rpx;
    display: flex;
    height: 50rpx;
    margin-left: 30rpx;
    width: 50rpx;
}

.current-url-address {
    color: #666;
    font-size: 26rpx;
    margin: 34rpx 0rpx 0rpx 32rpx;
}

.dot {
    margin: 0rpx 10rpx;
}