.container-source-main {
    flex-direction: row;
    padding-bottom: 28rpx;
    padding-left: 48rpx;
    padding-right: 48rpx;
    width: 100%;
}

.container-source-item,.container-source-main {
    box-sizing: border-box;
    display: flex;
}

.container-source-item {
    color: #fff;
    flex: 1;
    flex-direction: column;
    font-size: 36rpx;
    font-weight: 500;
    padding-bottom: 34rpx;
    text-align: center;
}

.txt-source-item {
    color: hsla(0,0%,100%,.7);
    font-size: 24rpx;
    letter-spacing: 2rpx;
    line-height: 24rpx;
    text-align: center;
    width: 100%;
}

.container-title {
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    padding: 58rpx 30rpx 46rpx;
    width: 100%;
}

.txt-title-main {
    color: #fff;
    font-size: 34rpx;
    font-weight: 700;
    letter-spacing: 2rpx;
    line-height: 40rpx;
    text-align: center;
    width: 100%;
}

.btn-bottom-hover {
    background: #0f1012;
}

