.output-audio {
    align-items: center;
    display: flex;
    flex-direction: row;
    float: right;
    height: 70rpx;
    justify-content: flex-end;
    margin-right: 20rpx;
    margin-top: -20rpx;
    width: 10%;
}

.output-audio .audio {
    border-radius: 10rpx;
    overflow: hidden;
    padding-right: 40rpx;
}

.output-audio .audio .ico {
    height: 70rpx;
    width: 70rpx;
}

.output-audio .audio .ico1 {
    height: 70rpx;
    transform: scale(2.3);
    width: 70rpx;
}

.output-audio .audio .time {
    float: right;
    font-size: 24rpx;
}

.badge-left-voice-on {
    background: url("data:image/png;base64,");
    background-size: 70rpx 70rpx;
}

.badge-left-voice-close,.badge-left-voice-on {
    float: left;
    height: 70rpx;
    margin-left: 0rpx;
    margin-top: 13rpx;
    width: 70rpx;
}

.badge-left-voice-close {
    background: url("data:image/png;base64,");
    background-size: 70rpx 70rpx;
}