.wrapper {
    align-items: center;
    height: 100%;
    justify-content: center;
    margin: 0 auto;
    width: 87%;
}

.upBlock,.wrapper {
    display: flex;
}

.upBlock {
    background-color: #fff;
    border-radius: 6px 6px 6px 6px;
    flex-direction: column;
    height: auto;
    width: 90%;
}

.titleBlock {
    flex: 1;
    flex-direction: row;
}

.left-icon-block,.titleBlock {
    display: flex;
}

.left-icon-block {
    flex: 0.35;
}

.title {
    align-items: center;
    flex-direction: column;
    justify-content: flex-start;
    padding-top: 50rpx;
}

.title,.titleText {
    display: flex;
    flex: 1;
}

.titleText {
    color: #333;
    font-size: 34rpx;
    font-weight: 700;
}

.right-icon-block {
    display: flex;
    flex: 0.35;
}

.overlay-left-icon {
    background: url("data:image/png;base64,");
    background-size: 200rpx 200rpx;
    height: 200rpx;
    width: 200rpx;
}

.overlay-right-icon {
    background: url("data:image/png;base64,");
    background-size: 200rpx 200rpx;
    height: 200rpx;
    width: 200rpx;
}

.overlay-content-text-block {
    align-items: center;
    display: flex;
    flex: 1;
    flex-direction: column;
    margin-top: -60rpx;
}

.numberText {
    display: flex;
    flex: 1;
    flex-direction: row;
    font-family: Oswald-SemiBold,Oswald;
    font-size: 40rpx;
    padding-top: 10rpx;
}

.sign-in-color {
    color: #f5a200;
}

.voucher-color {
    color: #000;
}

.voucher-text {
    color: #999;
    display: flex;
    flex: 1;
    font-size: 24rpx;
    padding-top: 10rpx;
}

.coin {
    background: url("data:image/svg+xml;base64,");
    background-size: 120rpx 120rpx;
    height: 120rpx;
    width: 120rpx;
}

.coupon {
    background: url("data:image/png;base64,");
    background-size: 100rpx 100rpx;
    height: 100rpx;
    width: 100rpx;
}

.overlay-content-icon-block {
    align-items: center;
    justify-content: center;
    margin-top: 28rpx;
}

.overlay-btn,.overlay-content-icon-block {
    display: flex;
    flex: 1;
}

.overlay-btn {
    margin-bottom: 60rpx;
    margin-top: 40rpx;
}

.public-style {
    border-radius: 20px 20px 20px 20px;
    font-size: 30rpx;
    width: 50%;
}

.sign-in-btn {
    background: #f5a200;
    color: #fff;
}

.voucher-btn {
    background: #fff;
    border: 2rpx solid #6e7480;
    color: #666;
}

button::after {
    border: none;
}

button[disabled] {
    color: #fff!important;
}