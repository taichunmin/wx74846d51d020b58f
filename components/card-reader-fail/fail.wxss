page {
    height: calc(100% - constant(safe-area-inset-bottom));
    height: calc(100% - env(safe-area-inset-bottom));
    overflow: hidden;
    width: 100%;
}

.contaniner-fail-main {
    overflow: hidden;
}

.container-err-main,.contaniner-fail-main {
    display: flex;
    flex-direction: column;
    height: 100%;
    width: 100%;
}

.container-err-main {
    flex: 5;
    overflow: auto;
    text-align: center;
}

.container-top-title {
    align-items: center;
    display: flex;
    flex: 1;
    flex-direction: column;
    justify-content: center;
    padding-left: 60rpx;
    padding-right: 60rpx;
}

.txt-main-title {
    color: #333;
    font-size: 34rpx;
    font-weight: 700;
    letter-spacing: 2rpx;
    margin-top: 30rpx;
}

.container-error-content {
    box-sizing: border-box;
    padding-left: 68rpx;
    padding-right: 68rpx;
    width: 100%;
}

.container-error-content view {
    margin-top: 20rpx;
}

.txt-fail-title {
    color: #333;
    font-weight: 700;
    left: 0rpx;
}

.txt-fail-msg,.txt-fail-title {
    font-size: 26rpx;
    text-align: left;
}

.txt-fail-msg {
    color: #666;
    font-weight: 400;
    left: 2rem;
    margin-top: 24rpx;
}

.icon-fail-img {
    background: url("data:image/svg+xml;base64,");
    background-size: 200rpx 200rpx;
    height: 200rpx;
    width: 200rpx;
}

.container-tag-info-main {
    align-items: center;
    background: #f9f9f9;
    border-radius: 12rpx;
    box-sizing: border-box;
    display: flex;
    margin-top: 40rpx;
    opacity: 1;
    padding: 24rpx;
    width: 100%;
}

.container-tag-info {
    display: flex;
    flex-direction: column;
}

.container-tag-info,.txt-tag-info {
    width: max-content;
}

.txt-tag-info {
    color: #666;
    font-size: 26rpx;
    font-weight: 400;
    line-height: 40rpx;
    text-align: center;
}

.icon-right-double-arrow {
    background: url("data:image/svg+xml;base64,");
    background-size: 40rpx 40rpx;
    height: 40rpx;
    margin-top: 0rpx;
    position: absolute;
    width: 40rpx;
}

.container-retry-btn {
    align-items: center;
    box-sizing: border-box;
    display: flex;
    flex: 2;
    flex-direction: row;
    justify-content: center;
    padding-left: 40rpx;
    padding-right: 40rpx;
    width: 100%;
}

.btn-retry-opera {
    border-radius: 40rpx;
    font-size: 30rpx;
    font-weight: 500;
    height: 80rpx;
    width: 100%;
}

.empty-card {
    background: hsla(0,0%,100%,.9);
    border: 1px solid #333;
    color: #333;
    margin-right: 40rpx;
}

.retry {
    background: #f5a200;
    border: 1px solid #f5a200;
    color: #fff;
    margin-left: 40rpx;
}

.btn-retry-opera::after {
    border: none;
}

.container-help-question {
    color: #f5a200;
    flex: 0.8;
    font-size: 28rpx;
    text-align: center;
    width: 100%;
}